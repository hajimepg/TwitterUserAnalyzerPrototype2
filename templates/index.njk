        <div id="container">
            <h1 id="name">hajimepg はじぴー</h1>

            <div id="icon">
                <img src="icon.jpg">
            </div>

            <div id="basicInfo">
                <dl>
                    <dt>Tweets</dt>
                    <dd>489,145<span class="subInfo">(152/Day)<span></dd>

                    <dt>Following</dt>
                    <dd>792<span class="subInfo">(123 Mutual Following)</span></dd>

                    <dt>Follower</dt>
                    <dd>1698<span class="subInfo">(234 Mutual Followers)</span></dd>

                    <dt>Join</dt>
                    <dd>Apr 2007</dd>

                    <dt>Bio</dt>
                    <dd>
                        フィギュアレビューブログはじめました<br/>
                        http://figure.studiohff.net/
                    </dd>
                </dl>
            </div>

            <div id="mentions">
                <h1>Mentions</h1>
                <table><tbody>
                    {% for item in replyTweetCount %}
                        <tr>
                            <td><a href="https://twitter.com/{{ item.screen_name }}" target="_blank" class="userNameLink">@{{ item.screen_name }}</a></td>
                            <td>{{ item.count }}</td>
                        </tr>
                    {% endfor %}
                </tbody></table>
            </div>

            <div id="hashtags">
                <h1>Hashtags</h1>
                <table><tbody>
                    {% for item in hashtagTweetCount %}
                        <tr>
                            <td><span class="hashtagLink">#{{ item.hashtag }}</span></td>
                            <td>{{ item.count }}</td>
                        </tr>
                    {% endfor %}
                </tbody></table>
            </div>

